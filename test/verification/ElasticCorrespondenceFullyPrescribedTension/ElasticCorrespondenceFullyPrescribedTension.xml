<ParameterList>
  
  <ParameterList name="Discretization">
	<Parameter name="Type" type="string" value="Exodus" />
	<Parameter name="Input Mesh File" type="string" value="ElasticCorrespondenceFullyPrescribedTension.g"/>
  </ParameterList>

  <ParameterList name="Materials">
	<ParameterList name="My Elastic Correspondence Material">
	  <Parameter name="Material Model" type="string" value="Elastic Correspondence"/>
	  <Parameter name="Density" type="double" value="7800.0"/>
	  <Parameter name="Young's Modulus" type="double" value="200.0e9"/>
	  <Parameter name="Poisson's Ratio" type="double" value="0.0"/>   <!-- One-dimensional simulation -->
	</ParameterList>
  </ParameterList>

  <ParameterList name="Blocks">
	<ParameterList name="My Group of Blocks">
	  <Parameter name="Block Names" type="string" value="block_1 block_2 block_3"/>
	  <Parameter name="Material" type="string" value="My Elastic Correspondence Material"/>
          <Parameter name="Horizon" type="double" value="0.751"/>
	</ParameterList>
  </ParameterList>

  <ParameterList name="Boundary Conditions">

        <!-- Fully prescribe all displacement degrees of freedom -->
        <!-- 0.1% strain in y direction, zero displacement is x and z directions -->
	<ParameterList name="Prescribed Displacement X">
	  <Parameter name="Type" type="string" value="Prescribed Displacement"/>
	  <Parameter name="Node Set" type="string" value="nodelist_1"/>
	  <Parameter name="Coordinate" type="string" value="x"/>
	  <Parameter name="Value" type="string" value="0.0"/>
	</ParameterList>
	<ParameterList name="Prescribed Displacement Y">
	  <Parameter name="Type" type="string" value="Prescribed Displacement"/>
	  <Parameter name="Node Set" type="string" value="nodelist_1"/>
	  <Parameter name="Coordinate" type="string" value="y"/>
	  <Parameter name="Value" type="string" value="0.001*y*t/1.0e-8"/>
	</ParameterList>
	<ParameterList name="Prescribed Displacement Z">
	  <Parameter name="Type" type="string" value="Prescribed Displacement"/>
	  <Parameter name="Node Set" type="string" value="nodelist_1"/>
	  <Parameter name="Coordinate" type="string" value="z"/>
	  <Parameter name="Value" type="string" value="0.0"/>
	</ParameterList>

  </ParameterList>

  <ParameterList name="Solver">
	<Parameter name="Verbose" type="bool" value="false"/>
	<Parameter name="Initial Time" type="double" value="0.0"/>
	<Parameter name="Final Time" type="double" value="1.0e-8"/>
	<ParameterList name="Verlet">
	  <Parameter name="Fixed dt" type="double" value="2.0e-9"/>
	</ParameterList>
  </ParameterList>

  <!-- Track the displacement for the ends of a strain gage       -->
  <!-- Track the reaction forces at the top and bottom of the bar -->
  <ParameterList name="Compute Class Parameters">
     <ParameterList name="Top Reaction Force">
       <Parameter name="Compute Class" type="string" value="Block_Data"/>
       <Parameter name="Calculation Type" type="string" value="Sum"/>
       <Parameter name="Block" type="string" value="block_3"/>
       <Parameter name="Variable" type="string" value="Force"/>
       <Parameter name="Output Label" type="string" value="Top_Reaction_Force"/>
     </ParameterList>
     <ParameterList name="Bottom Reaction Force">
       <Parameter name="Compute Class" type="string" value="Block_Data"/>
       <Parameter name="Calculation Type" type="string" value="Sum"/>
       <Parameter name="Block" type="string" value="block_1"/>
       <Parameter name="Variable" type="string" value="Force"/>
       <Parameter name="Output Label" type="string" value="Bottom_Reaction_Force"/>
     </ParameterList>
  </ParameterList>

  <ParameterList name="Output">
	<Parameter name="Output File Type" type="string" value="ExodusII"/>
	<Parameter name="Output Filename" type="string" value="ElasticCorrespondenceFullyPrescribedTension"/>
	<Parameter name="Output Frequency" type="int" value="1"/>
	<ParameterList name="Output Variables">
          <Parameter name="Volume" type="bool" value="true"/>
          <Parameter name="Deformation_GradientXX" type="bool" value="true"/>
          <Parameter name="Deformation_GradientXY" type="bool" value="true"/>
          <Parameter name="Deformation_GradientXZ" type="bool" value="true"/>
          <Parameter name="Deformation_GradientYX" type="bool" value="true"/>
          <Parameter name="Deformation_GradientYY" type="bool" value="true"/>
          <Parameter name="Deformation_GradientYZ" type="bool" value="true"/>
          <Parameter name="Deformation_GradientZX" type="bool" value="true"/>
          <Parameter name="Deformation_GradientZY" type="bool" value="true"/>
          <Parameter name="Deformation_GradientZZ" type="bool" value="true"/>
          <Parameter name="StrainXX" type="bool" value="true"/>
          <Parameter name="StrainXY" type="bool" value="true"/>
          <Parameter name="StrainXZ" type="bool" value="true"/>
          <Parameter name="StrainYX" type="bool" value="true"/>
          <Parameter name="StrainYY" type="bool" value="true"/>
          <Parameter name="StrainYZ" type="bool" value="true"/>
          <Parameter name="StrainZX" type="bool" value="true"/>
          <Parameter name="StrainZY" type="bool" value="true"/>
          <Parameter name="StrainZZ" type="bool" value="true"/>
          <Parameter name="StressXX" type="bool" value="true"/>
          <Parameter name="StressXY" type="bool" value="true"/>
          <Parameter name="StressXZ" type="bool" value="true"/>
          <Parameter name="StressYX" type="bool" value="true"/>
          <Parameter name="StressYY" type="bool" value="true"/>
          <Parameter name="StressYZ" type="bool" value="true"/>
          <Parameter name="StressZX" type="bool" value="true"/>
          <Parameter name="StressZY" type="bool" value="true"/>
          <Parameter name="StressZZ" type="bool" value="true"/>
          <Parameter name="Displacement" type="bool" value="true"/>
          <Parameter name="Velocity" type="bool" value="true"/>
          <Parameter name="Force" type="bool" value="true"/>
          <Parameter name="Top_Reaction_Force" type="bool" value="true"/>
          <Parameter name="Bottom_Reaction_Force" type="bool" value="true"/>
	</ParameterList>
  </ParameterList>
  
</ParameterList>
